{
  "name": "@twiddle/workflows",
  "version": "0.2.0",
  "private": true,
  "description": "Temporal workflow definitions for Twiddle",
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    }
  },
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "clean": "rm -rf dist tsconfig.tsbuildinfo",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "@twiddle/shared": "workspace:*",
    "@temporalio/workflow": "^1.11.0"
  },
  "devDependencies": {
    "typescript": "^5.7.0"
  }
}
